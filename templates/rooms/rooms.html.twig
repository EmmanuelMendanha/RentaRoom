{% extends 'base.html.twig' %}

{% block title %}Rooms - RentaRoom{% endblock %}

{% block body %}
    <h1>Rooms</h1>

    {% for room in rooms %}
    <div>
        <h2><a href="{{ path('room_show', {id: room.id}) }}">{{ room.title }}</a></h2>
        <p>{{ room.description }}</p>
        <p>Price: {{ room.price }}</p>
        <img src="{{ room.imageMain }}" alt="{{ room.title }}">
    </div>
{% endfor %}
{% endblock %}
